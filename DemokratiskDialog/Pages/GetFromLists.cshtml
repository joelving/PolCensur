@page
@model DemokratiskDialog.Pages.GetFromListsModel
@{
    ViewData["Title"] = "GetFromLists";
}

<h1>GetFromLists</h1>

<p>I alt @Model.Profiles.Count profiler</p>

@foreach (var group in Model.Profiles)
{
    <h2>@group.Key (i alt @group.Value.Count())</h2>
    foreach (var profile in group.Value)
    {
        <div>@profile.IdStr:    @profile.ScreenName</div>
    }
}